vector<int> solution(vector<int> array, vector<vector<int>> commands) {
    vector<int> answer;
    for(auto a : commands)
    {
        int l = a.at(0);
        int m = a.at(1);
        int n = a.at(2);
        vector<int> temp;
        for(int k = l ; k <= m; k++)
        {
            temp.push_back(array.at(k-1));
        }
        sort(temp.begin(), temp.end());
        answer.push_back(temp.at(n-1));
    }
    return answer;
}
